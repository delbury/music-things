{
  "name": "music-things",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --fix --config .eslintrc.js --ext .js,.ts,.vue ./",
    "cm": "cz"
  },
  "dependencies": {
    "vue": "^3.0.11",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.39",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "dart-sass": "^1.25.0",
    "eslint": "^7.24.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "sass": "^1.32.8",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vue-tsc": "0.0.23"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{css,less,scss,vue}": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
